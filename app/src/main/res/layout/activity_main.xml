<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <LinearLayout
        android:id="@+id/linearColumnharyparker"
        style="@style/groupStylegray_50"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_56pxv"
        android:gravity="start"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginEnd="@dimen/_24pxh"
            android:gravity="center_horizontal"
            app:contentInsetStart="0dp">

            <LinearLayout
                android:id="@+id/linearRowharyparker"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtHaryParker"
                    style="@style/txtMontserratbold24"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_weight="0.77"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_wm"
                    tools:text="@string/lbl_wm" />

                <ImageButton
                    android:id="@+id/btnSearch"
                    android:layout_width="@dimen/_44pxh"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_133pxh"
                    android:background="@drawable/rectangle_bg_white_a700_radius_22"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" />
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

        <androidx.core.widget.NestedScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_weight="1"
            android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="end">

            <LinearLayout
                android:id="@+id/linearColumnnewplace"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/linearRownewplace"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtNewPlace"
                        style="@style/txtMontserratsemibold18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.57"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_venue"
                        tools:text="@string/lbl_venue" />

                    <LinearLayout
                        android:id="@+id/linearRowseemore"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_151pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.43"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txtSeemore"
                            style="@style/txtMontserratmedium12"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.9"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_see_more"
                            tools:text="@string/lbl_see_more" />

                        <ImageView
                            android:id="@+id/imageOffer"
                            android:layout_width="@dimen/_6pxh"
                            android:layout_height="@dimen/_8pxh"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_offer"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_offer" />
                    </LinearLayout>
                </LinearLayout>

                <HorizontalScrollView
                    android:id="@+id/horizontalScrollHorizontalscrollvenue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:fillViewport="true"
                    android:scrollbars="none">

                    <LinearLayout
                        android:id="@+id/venueLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageRectangleFifteen"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle15}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1875_1" />

                        <ImageView
                            android:id="@+id/imageRectangleSixteen"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1899_1" />
                        <ImageView
                            android:id="@+id/imageRectangle17"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1899_1" />
                        <ImageView
                            android:id="@+id/imageRectangle18"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle17}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1899_1" />
                        <ImageView
                            android:id="@+id/imageRectangle19"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle17}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1899_1" />
                    </LinearLayout>
                </HorizontalScrollView>

                <LinearLayout
                    android:id="@+id/linearRowpopurlarhotelmenu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtPopurlarHotelmenu"
                        style="@style/txtMontserratsemibold18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.65"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_menu"
                        tools:text="@string/lbl_menu" />
                </LinearLayout>

                <HorizontalScrollView
                    android:id="@+id/horizontalScrollHorizontalscrollfood"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:fillViewport="true"
                    android:scrollbars="none">

                    <LinearLayout
                        android:id="@+id/food_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageRectangleFifteenmenu"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle15}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1662" />

                        <ImageView
                            android:id="@+id/imageRectangleSixteenmenu"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1669" />
                        <ImageView
                            android:id="@+id/imageRectangle17menu"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1669" />
                        <ImageView
                            android:id="@+id/imageRectangle18menu"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1669" />
                        <ImageView
                            android:id="@+id/imageRectangle19menu"
                            cornerRadius="@{@dimen/_20pxh}"
                            imageRes="@{@drawable/img_rectangle16}"
                            android:layout_width="@dimen/_300pxh"
                            android:layout_height="@dimen/_160pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/lnom1669" />
                    </LinearLayout>
                </HorizontalScrollView>

                <LinearLayout
                    android:id="@+id/linearRowpopurlarhotel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtPopurlarHotel"
                        style="@style/txtMontserratsemibold18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.65"
                        android:ellipsize="end"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@string/lbl_order"
                        tools:text="@string/lbl_order" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <Button
                        android:id="@+id/orderBtn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Start your order" />
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>

        <FrameLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStylegray_51cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/linearRowalarm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_marginBottom="@dimen/_43pxv"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageAlarm"
                    android:layout_width="@dimen/_17pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_alarm"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_alarm" />

                <ImageView
                    android:id="@+id/imageUser"
                    android:layout_width="@dimen/_15pxh"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_user" />

                <ImageView
                    android:id="@+id/imageHomeOne"
                    android:layout_width="@dimen/_19pxh"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_20x19"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_home_20x19" />

                <ImageView
                    android:id="@+id/imageActivity"
                    android:layout_width="@dimen/_19pxh"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_57pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_activity"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_activity" />

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_19pxh"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_56pxh"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowleft" />
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
